{
  "name": "@vectry/js-core",
  "version": "1.0.7",
  "description": "Universal JavaScript SDK core for Vectry Analytics. Enables structured event capture, enrichment, validation, and transport across environments.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "dev": "tsup src/index.ts --dts --watch --format esm,cjs --out-dir dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "vectry",
    "event",
    "causal",
    "analytics",
    "observability",
    "sdk",
    "javascript",
    "typescript"
  ],
  "author": "Vectry Anlytics",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
